<p id="notice"><%= notice %></p>





<h1 class="project-name">
<% if @submission.project %>
There is a project
<% else %>
No Project
<% end %>
</h1>

<% #if there is no submission.code, write an error %>
<div class="code">
<% if @submission.code and @submission.code.entry %>
<pre>
<%= @submission.code.entry %>
</pre>
<% else %>
Error, no code found
<% end %>
</div>


<div class="expected-output">
<h3>Expected Output</h3>
<% if @submission.code.expected_output %>
<pre>
<%= @submission.code.expected_output %>
</pre>
<% else %>
<div class="expected-output no-expected-output">
No Expected Output
</div>
<% end %>
</div>





<p>
<b>Rating:</b>
<% if @submission.rating %>
<%= @submission.rating %>
<% else %>
No Rating
<% end %>
</p>


